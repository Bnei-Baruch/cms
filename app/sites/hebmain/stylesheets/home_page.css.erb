/***********  BODY  ****************/
#bd .yui-ge{
	margin:0 15px 0 6px;
}
/*----------  h1  ---------------*/
.h1{
	background-color:#0D47B2;
	color:white;
	text-align:right;
	font-size:153.9%;
	font-weight:bold;
	margin:0;
	background: #0D47B2 url(<%= domain %>/images/<%= @site_name %>/top-mid.gif) repeat-x bottom right;
	height:41px;
	line-height:41px;
  position:relative;
  padding: 0 10px 0 0;
}
.h1 .h1-right{
	position:absolute;
  top:0;
  right:0;
}
.h1 .h1-left{
	position:absolute;
  top:0;
  left:0;
}
/*----------  breadcrumbs  ---------------*/
#bd .breadcrumbs{
	text-align:right;
	padding:0 10px;
	margin:0 0 25px;
	height:30px;
	line-height:30px;
	border-bottom:1px solid #e8e8e8;
}
#bd .breadcrumbs .gt{
	color:#b5b5b5;
}

/*----------  Content  ---------------*/
.content {
	margin:0;
	padding:0;
	text-align:right;
	position: relative;
  width:99%;
}
.content h1{
	font-size:22px;
	color:#0D47B2;
	margin:0;
	padding:0;
}
.content h2{
	font-size:16px;
	margin:0 0 30px;
	padding:0;
}
.content h3{
	font-size:16px;
	color:#192862;
	margin-bottom:3px;
}
.content .author{
	height:18px;
	margin: 5px 0;
}
.content .descr{
	font-size:16px;
	color:#696969;
	margin:0;
	padding:0 0 25px;
	border-bottom:1px solid #e8e8e8;
}
.content .item{
	font-size:14px;
	margin:0 0 25px 0;
	position: relative;
}
.content .item p{
	margin-bottom:15px;
}
.content .image{
	background-color:#E8E8E9;
	margin:0 0 20px 20px;
	padding: 5px;
	float:right;
}
.content .full-image{
	background-color:#E8E8E9;
	margin:0 auto 10px;
	padding: 5px;
	text-align:center;
}
.content .item h3.video {
	background: transparent url(<%= domain %>/images/<%= @site_name %>/player/top.gif) no-repeat top right;
	padding:7px 10px 5px 0;
	margin:1em 0 0;
}
.content .item .full-video {
	background: transparent url(<%= domain %>/images/<%= @site_name %>/player/bg.gif) repeat-y top right;
}
.content .item .full-video p{
	padding:10px 10px 15px;
	margin:0;
}
.content .item .embed{
	background: transparent url(<%= domain %>/images/<%= @site_name %>/player/bottom.gif) no-repeat top right;
	padding:10px 8px 5px 20px;
	margin:0;
	text-align:left;
	direction:ltr;
	float:right;
}
.content .item .embed span.text{
	font-weight:bold;
	padding:0 5px;
}
.content .item .embed span.services{
	padding: 0 0 0 10px;
}
.content .item .embed span.services img{
	padding: 0 0 0 5px;
	vertical-align:middle;
}
.content .item .embed input{
	width:300px;
	text-align:left;
	border:1px solid #b2c4e4;
}
.content .item .video {
	margin:25px 0 0;
}
.content .item .video img {
	vertical-align: middle;
	padding: 0 0 0 10px;
}
.content .clear {
	clear: both;
}
/*----------  Section Preview  ---------------*/
.section_preview{
  padding:0;
	margin:25px 0 0;
  position:relative;
}
.section_preview .descr{
  font-size:12px;
  color:black;
  border:0 none;
  padding:0;
}
.section_preview .author{
	height:12px;
	margin: 15px 0;
  font-size:12px;
  color:#696969;
}
.section_preview .element{
  padding: 0 158px 0 20px;
  position:relative;
  border-top:1px dashed #e8e8e8;
}
.section_preview .element h1{
  font-size: 16px;
  padding: 15px 0;
}
.section_preview .img{
  position:absolute;
  right:0px;
  top:15px;
}
.section_preview .footer{
  background-color:#f6f8fb;
  height:35px;
  line-height:35px;
  border-bottom:1px solid #e8e8e8;
  padding:0 0 0 10px;
}
.section_preview .footer img{
  padding:0 5px 0 0;
  vertical-align:middle;
}
/*----------  Main Preview  ---------------*/
.main_preview1{
  margin:0 0 0 250px;
  position:relative;
}
.main_preview1 .descr{
  font-size:14px;
  color:black;
  border:0 none;
  padding:0 0 20px 0;
  border:0 none;
}
.main_preview1 .author{
	height:18px;
	margin: 5px 0;
}
.main_preview1 h1{
  font-size: 18px;
  padding: 10px 0 0;
}
.main_preview1 h2{
  margin: 0 0 10px;
}
.main_preview1 img{
  position:absolute;
  top:15px;
  right:223px;
  *right:208px;
}
/*----------  Main Preview 2 ---------------*/
.main_preview2{
  background:transparent url(<%= domain %>/images/<%= @site_name %>/main_preview2-bg.gif) repeat-y 50% top;
}
.main_preview2 .element{
  float:left;
  position:relative;
  width:45%;
  padding:176px 0 0;
  margin:0 44px 0 0;
}
.main_preview2 .last{
  margin:0;
}
.main_preview2 .descr{
  font-size:12px;
  color:black;
  border:0 none;
  padding:0 0 20px 0;
  border:0 none;
}
.main_preview2 .author{
	height:12px;
	margin: 15px 0;
  font-size:12px;
  color:#696969;
}
.main_preview2 h1{
  font-size: 18px;
  padding: 27px 0pt 33px;
}
.main_preview2 img{
  width:204px;
  position:absolute;
  left:0px;
  top:15px;
}
/*----------  Main Preview 3 ---------------*/
.main_preview3{
  border:1px solid #e5e5e5;
  margin:10px 0;
  padding:0;
  text-align:right;
}
.main_preview3 .element{
  float:left;
  position:relative;
  width:30%;
  padding:5px 5px 20px;
  margin:0;
  border-right:1px dashed #e5e5e5;
}
.main_preview3 .element h3 {
	color:black;
	font-size:153.9%;
	font-weight:bold;
	background-color: #f0f5fd;
	height:45px;
	line-height:45px;
  margin: 0 0 17px;
  padding:0 10px 0 0;
}
.main_preview3 .last{
  border:0 none;
}
.main_preview3 .descr{
  font-size:12px;
  color:black;
  border:0 none;
  padding:0 0 20px 0;
  border:0 none;
}
.main_preview3 h4{
  font-size: 12px;
  padding: 0 0pt 35px;
}
.main_preview3 img{
  margin:0 12px 17px;
  width:114px;
}
.main_preview3 .element ul.links{
  padding:0;
  margin:0;
}
.main_preview3 .element ul.links li{
  font-size:12px;
	text-align: right;
	list-style-position: inside;
	list-style-type:none;
	padding:0 13px 0 0;
	margin:3px 0 0;
	*padding:0;
}
/*----------  right-part  ---------------*/
.right-part {
	text-align: center;
	margin:56px 0 0;
	padding:0;
        position:relative;
}
.right-part .player {
  padding:5px 0 20px;
  margin:0;
	background-color: #F0F4FD;
}
.right-part .player ul{
  padding:20px 0 0;
  margin:0;
}
.right-part .player ul li{
	text-align: right;
	list-style-position: inside;
	list-style-type:none;
	padding:0 13px 0 0;
	margin:3px 0 0;
	*padding:0;
}
.right-part .player .more{
  margin:23px 0 0;
}
.right-part .updates{
  border:1px solid #e5e5e5;
  margin:10px 0;
  padding:5px;
  text-align:right;
  position:relative;
}
.right-part .updates h3 {
	color:black;
	font-size:153.9%;
	font-weight:bold;
	background-color: #f0f5fd;
	height:45px;
	line-height:45px;
  margin: 0 0 22px;
  padding:0 10px 0 0;
}
.right-part .update{
  margin:0 0 20px;
  padding:0 5px 20px;
  border-bottom:1px dashed #e5e5e5;
  position:relative;
}
.right-part .last{
  border-bottom:0 none !important;
  margin:0;
}
.right-part .update h4 {
  color:#0e46b3;
  font-size:16px;
  padding:0 0 10px;
}
.right-part .update .link {
  padding:5px 0 0;
}
.right-part .update .link img {
  vertical-align: middle;
  padding:0 5px 0 0;
}
/*----------  left-part  ---------------*/
.left-part.left-part {
    position:relative;
}

.left-part .radio-TV{
  direction: ltr;
  background-color:#e9f1ff;
  padding:10px 0 0;
  margin:0;
}
.left-part .radio-TV ul li{
  list-style-type: none;
  text-align:center;
}
.left-part .body{
  direction: rtl;
  border:1px solid #e5e5e5;
  border-top:0 none;
  height:40px;
  text-align:center;
}
.left-part #tv{
  height:170px;
  padding:2px 0 0;
}
.left-part #radio{
  height:100px;
  padding:2px 0 0;
}
.left-part .rss{
  border:1px solid #e5e5e5;
  margin:10px 0;
  padding:5px;
  text-align:right;
  position:relative;
}
.left-part .rss h3 {
	color:black;
	font-size:153.9%;
	font-weight:bold;
	background-color: #f0f5fd;
	height:45px;
	line-height:45px;
  margin: 0 0 22px;
  padding:0 43px 0 0;
  position:relative;
}
.left-part .rss h3 img{
  position:absolute;
  right:5px;
  top:5px;
}
.left-part .rss .entry{
  color:#767676;
  margin:0 0 10px 0;
}
.left-part .rss .entry .date{
  color:#767676;
  font-size:13px;
}
.left-part .rss .more{
  height:27px;
  border-top:1px dashed #bbbbbb;
  display:block;
}
.left-part .downloads{
  border:1px solid #e5e5e5;
  margin:10px 0;
  padding:5px;
  text-align:right;
}
.left-part .downloads h3 {
	color:black;
	font-size:153.9%;
	font-weight:bold;
	background-color: #f0f5fd;
	height:45px;
	line-height:45px;
  margin: 0 0 22px;
  padding:0 43px 0 0;
  position:relative;
}
.left-part .x-tab-strip span.x-tab-strip-text{
  color:black;
}
.left-part .x-tab-strip-top .x-tab-right,
.left-part .x-tab-strip-top .x-tab-left,
.left-part .x-tab-strip-top .x-tab-strip-inner {
  background-image:url(<%= domain %>/images/<%= @site_name %>/ext/tabs-sprite.gif);
}
.left-part .x-tab-panel-header, .left-part .x-tab-panel-footer {
background:#ffffff none repeat scroll 0% 50%;
border-color:#D0D0D0;
}
.left-part ul.x-tab-strip-top {
background:#E9F1FF none;
}
.left-part .x-tab-panel-header {
padding-bottom:0;
border:0 none;
}




