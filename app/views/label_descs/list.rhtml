<h1>Listing label_descs</h1>

<table>
  <tr>
  <% for column in LabelDesc.content_columns %>
    <th><%= column.human_name %></th>
  <% end %>
  </tr>
  
<% for label_desc in @label_descs %>
  <tr>
  <% for column in LabelDesc.content_columns %>
    <td><%=h label_desc.send(column.name) %></td>
  <% end %>
    <td><%= link_to 'Show', :action => 'show', :id => label_desc %></td>
    <td><%= link_to 'Edit', :action => 'edit', :id => label_desc %></td>
    <td><%= link_to 'Destroy', { :action => 'destroy', :id => label_desc }, :confirm => 'Are you sure?', :method => :post %></td>
  </tr>
<% end %>
</table>

<%= link_to 'Previous page', { :page => @label_desc_pages.current.previous } if @label_desc_pages.current.previous %>
<%= link_to 'Next page', { :page => @label_desc_pages.current.next } if @label_desc_pages.current.next %> 

<br />

<%= link_to 'New label_desc', :action => 'new' %>
