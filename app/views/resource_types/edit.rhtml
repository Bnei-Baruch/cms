<% @title = "Editing resource type" %>
<% form_for(:resource_type, :url => resource_type_path(@resource_type), :html => {:method => 'put'}) do |f| %>
	<p>
	<%= render :partial => 'fields', :locals => {:f => f} %>
	<span id="submit"><%= submit_tag "Update Resource Type" %></span>
 </p>
<% end %>
<%= link_to 'Show', resource_type_path(@resource_type) %> |
<%= link_to 'Back', resource_types_path %>